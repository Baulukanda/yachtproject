extends layout

block content
	link(rel='stylesheet', href='/stylesheets/links.css')
	h1= 'Links'
	each link in links
		a(href=link.href)
			div(class="linkWrapper container")
				span(class="row")
					img(src=link.image class="col-2")
					span(class="col-6")
						h4(class="row")=link.title
						p(class="row")=link.desc
		br

block navbar
	+navbar(labels, navbarActiveSection)